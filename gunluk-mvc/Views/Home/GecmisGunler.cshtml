@model List<Icerik>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Baslık</th>
            <th scope="col">Günlüğüm</th>
            
        </tr>
    </thead>
    <tbody>
        
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Baslik</td>
                <td>@item.Gunlugum</td>
            </tr>
        }
    </tbody>
</table>



<p>Gecmis Günler Page.</p>
